<refentry lang="en">
  <refentryinfo>
    <title>GrandOrgue man page</title>
    <author>
      <surname>GrandOrgue contributors (see AUTHORS)</surname>
      <affiliation>
        <address>
          <email>ourorgan-developers@lists.sourceforge.net</email>
        </address>
      </affiliation>
