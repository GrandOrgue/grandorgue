#. type: Content of: <book><chapter><sect1><title>
#: grandorgue.xml:7085 grandorgue.xml:11574
msgid "Organ section"
msgstr ""

#. type: Content of: <book><chapter><sect1><para>
#: grandorgue.xml:7087
msgid ""
"This section describes the whole organ. For the old panel format, it includes the "
"main panel, therefore the section includes all display metrics attributes. The new "
"panel format separates the display metrics of the main panel into a section named "
"<emphasis>Panel000</emphasis>. Additionally it includes the following attributes:"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7096 grandorgue.xml:11589
msgid "ChurchName"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7099
msgid ""
"(string, required) Name of the organ/church. This string should be unique, as "
"setting files for organs with the same <emphasis>ChurchName</emphasis> are "
"considered compatible. GrandOrgue will not load a settings file if the ChurchName "
"does not match."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7107
msgid "ChurchAddress"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7110 grandorgue.xml:7118 grandorgue.xml:7126 grandorgue.xml:7134
#: grandorgue.xml:7142
msgid "(string, required) informational text displayed in the property dialog"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7115 grandorgue.xml:11598
msgid "OrganBuilder"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7123
msgid "OrganBuildDate"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7131
msgid "OrganComments"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7139 grandorgue.xml:11607
msgid "RecordingDetails"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7147
msgid "InfoFilename"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7150
msgid ""
"(string, not required) relative path to an html file with more information about "
"the organ. This setting is currently not supported for organ packages."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7156 grandorgue.xml:8469 grandorgue.xml:10923
msgid "NumberOfManuals"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7159
msgid ""
"(integer 1 - 16, required) number of manuals. It does not include the pedal "
"keyboard.  The manual information for each manual is available in sections called "
"<emphasis>Manual999</emphasis>. 999 is a number defining each manual, starting with "
"001."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7167 grandorgue.xml:10931 grandorgue.xml:11208
msgid "HasPedals"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7170
msgid ""
"(boolean, required) Determines if the pedal, which is defined as section "
"<emphasis>Manual000</emphasis>, is present."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7176 grandorgue.xml:10762 grandorgue.xml:10949
msgid "NumberOfEnclosures"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7179
msgid ""
"(integer 0 - 999 required) number of enclosures. The details of each enclosure are "
"contained in a section called <emphasis>Enclosure999</emphasis>"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7184 grandorgue.xml:8350 grandorgue.xml:8525 grandorgue.xml:8718
#: grandorgue.xml:10772 grandorgue.xml:10968
msgid "NumberOfTremulants"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7187
msgid ""
"(integer 0 - 999, required) number of tremulants. The details of each tremulant are "
"contained in a section called <emphasis>Tremulant999</emphasis>"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7192
msgid "NumberOfWindchestGroups"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7195
msgid ""
"(integer 1 - 999, required) number of windchests. The details of each windchest are "
"in a section called <emphasis>WindchestGroup999</emphasis>."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7200 grandorgue.xml:10987
msgid "NumberOfReversiblePistons"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7203
msgid ""
"(integer 0 - 32, required) number of reversible pistons. The details of each "
"reversible piston are in a section called <emphasis>ReversiblePiston999</emphasis>."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7208 grandorgue.xml:11026
msgid "NumberOfGenerals"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7211
msgid ""
"(integer 0 - 99, required) number of generals. The details are in a section called "
"<emphasis>General999</emphasis>."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7217 grandorgue.xml:8368 grandorgue.xml:11045
msgid "NumberOfDivisionalCouplers"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7220
msgid ""
"(integer 0 - 8, required) number of divisional couplers. The details are in a "
"section called <emphasis>DivisionalCoupler999</emphasis>."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7226
msgid "NumberOfPanels"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7229
msgid ""
"(integer 0 - 100, default: 0) number of additional panels. The details are in a "
"section called <emphasis>Panel999</emphasis>."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7235 grandorgue.xml:8359 grandorgue.xml:8535 grandorgue.xml:8737
#: grandorgue.xml:11007
msgid "NumberOfSwitches"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7238
msgid ""
"(integer 0 - 999, default: 0) number of switches. The details are in a section "
"called <emphasis>Switch999</emphasis>."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7244 grandorgue.xml:10570
msgid "NumberOfRanks"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7247
msgid ""
"(integer 0 - 999, default: 0) number of additional ranks. The details are in a "
"section called <emphasis>Rank999</emphasis>."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7253
msgid "DivisionalsStoreIntermanualCouplers"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7256
msgid ""
"(boolean, required) determines if divisionals store/change the state of associated "
"intermanual couplers."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7262
msgid "DivisionalsStoreIntramanualCouplers"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7265
msgid ""
"(boolean, required) determines if divisionals store/change the state of associated "
"intramanual couplers."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7271
msgid "DivisionalsStoreTremulants"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7274
msgid ""
"(boolean, required) determines if divisionals store/change the state of associated "
"tremulants."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7280
msgid "GeneralsStoreDivisionalCouplers"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7283
msgid ""
"(boolean, required) determines if divisionals store/change the state of divisional "
"couplers."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7289
msgid "CombinationsStoreNonDisplayedDrawstops"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7292
msgid ""
"(boolean, default: false) determines, if the state of read-only objects (that is "
"calculated based on the state of another objects)  is stored in divisionals, "
"generals and the setter."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7299 grandorgue.xml:11168 grandorgue.xml:11216
msgid "NumberOfImages"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7302
msgid ""
"(integer 0 - 999, default: 0) Number of images on the panel. The section of the "
"image GUI definitions are called <emphasis>Image999</emphasis>. This setting is not "
"supported for the new panel format."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7310 grandorgue.xml:10939
msgid "NumberOfSetterElements"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:7313
msgid ""
"(integer 0 - 999, default: 0) Number of setter elements on the panel. The section "
"of the GUI definitions are called <emphasis>SetterElement999</emphasis>. This "
"setting is not supported for the new panel format."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:7321 grandorgue.xml:11158
