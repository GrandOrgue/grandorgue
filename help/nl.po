#. type: Content of: <book><chapter><sect1><title>
#: grandorgue.xml:8314
msgid "Combination data store"
msgstr ""

#. type: Content of: <book><chapter><sect1><para>
#: grandorgue.xml:8316
msgid "This object is used to store one combination It has the following attributes:"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:8320 grandorgue.xml:8496
msgid "Protected"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:8323 grandorgue.xml:8499
msgid "(boolean, default: false) If true, the stored combination cannot be changed."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:8329 grandorgue.xml:8505 grandorgue.xml:8661 grandorgue.xml:11074
msgid "NumberOfStops"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:8332
msgid ""
"(integer 0 - number of stops defined in the ODF, required) Number of stop states "
"stored in this combination. The entries are called <emphasis>StopNumber999</"
"emphasis> and <emphasis>StopManual999</emphasis>."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:8339 grandorgue.xml:8515 grandorgue.xml:8680 grandorgue.xml:11102
msgid "NumberOfCouplers"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:8342
msgid ""
"(integer 0 - number of coupler defined in the ODF, required) Number of coupler "
"states stored in this combination. The entries are called "
"<emphasis>CouplerNumber999</emphasis> and <emphasis>CouplerManual999</emphasis>."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:8353
msgid ""
"(integer 0 - tremulant count, required) Number of tremulant states stored in this "
"combination. The entries are called <emphasis>TremulantNumber999</emphasis>."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:8362
msgid ""
"(integer 0 - switch count, default: 0) Number of switch states stored in this "
"combination. The entries are called <emphasis>SwitchNumber999</emphasis>."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:8371
msgid ""
"(integer 0 - divisional coupler count, required if storing of divisional coupler in "
"the generals is enabled) Number of divisional coupler state stored in this "
"combination. The entries are called <emphasis>DivisionalCouplerNumber999</emphasis>."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:8379
msgid "StopNumber999"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:8382
msgid ""
"(integer -manual stop count - manual stop count, required) Number of the stop on "
"the manual. If the value is negative, it is turned off, else turned on."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:8388
msgid "StopManual999"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:8391
msgid ""
"(integer manual number, required) Number of the manual, which contains the stop."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:8396
msgid "CouplerNumber999"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:8399
msgid ""
"(integer -999 - 999, required) Number of the coupler on the manual. If the value is "
"negative, it is turned off, else it is turned on."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:8405
msgid "CouplerManual999"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:8408
msgid ""
"(integer manual number, required) Number of the manual, which contains the coupler."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:8413
msgid "TremulantNumber999"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:8416
msgid ""
"(integer -tremulant count - tremulant count, required)  Number of the tremulant. If "
"the value is negative, it is turned off, else turned on."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:8423
msgid "SwitchNumber999"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:8426
msgid ""
"(integer -switch count - switch count, required)  Number of the switch. If the "
"value is negative, it is turned off, else it is turned on."
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><term>
#: grandorgue.xml:8433
msgid "DivisionalCouplerNumber999"
msgstr ""

#. type: Content of: <book><chapter><sect1><variablelist><varlistentry><listitem><para>
#: grandorgue.xml:8436
msgid ""
"(integer -divisional coupler count -divisional coupler count, required) Number of "
"the divisional coupler. If the value is negative, it is turned off, else it is "
"turned on."
msgstr ""

#. type: Content of: <book><chapter><sect1><title>
